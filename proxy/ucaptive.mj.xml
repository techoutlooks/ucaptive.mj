<uwsgi>
    <plugin>python</plugin>
    <vhost/>
    <socket>/run/uwsgi/apps/ucaptive.mj/socket</socket>
    <chmod-socket>666</chmod-socket>
    <uid>techu</uid>
    <gid>www-data</gid>
    <pythonpath>/srv/apps/ucaptive.mj/backend</pythonpath>
    <pythonpath>/srv/.Envs/ucaptive.mj/lib/python2.7/site-packages/</pythonpath>
    <wsgi-file>/srv/apps/ucaptive.mj/backend/ucaptive/wsgi.py</wsgi-file>
    <virtualenv>/srv/.Envs/ucaptive.mj</virtualenv>
    <logto>/srv/log/apps/ucaptive.mj/uwsgi/log</logto>
    <master/>
    <processes>4</processes>
    <harakiri>60</harakiri>
    <reload-mercy>8</reload-mercy>
    <cpu-affinity>1</cpu-affinity>
    <stats>/tmp/stats.socket</stats>
    <max-requests>2000</max-requests>
    <limit-as>2048</limit-as>
    <reload-on-as>256</reload-on-as>
    <reload-on-rss>192</reload-on-rss>
    <no-orphans/>
    <vacuum/>
</uwsgi>

